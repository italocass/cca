class AFD_ImparAs:
    def __init__(self):
        self.state = 0  

    def transition(self, symbol):
        if symbol == 'a':
            self.state = 1 - self.state  

    def accepts(self, string):
        for symbol in string:
            self.transition(symbol)
        return self.state == 1



afd = AFD_ImparAs()
print(afd.accepts("ab"))   
print(afd.accepts("aab"))  
