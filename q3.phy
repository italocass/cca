class AFD_DoisUns:
    def __init__(self):
        self.state = 0  

    def transition(self, symbol):
        if symbol == '1':
            self.state += 1

    def accepts(self, string):
        for symbol in string:
            self.transition(symbol)
        return self.state == 2



afd = AFD_DoisUns()
print(afd.accepts("110"))  
print(afd.accepts("111"))  
