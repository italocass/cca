class AFD_TerminaEm01:
    def __init__(self):
        self.state = 0  

    def transition(self, symbol):
        if self.state == 0:
            if symbol == '0':
                self.state = 0
            elif symbol == '1':
                self.state = 1
        elif self.state == 1:
            if symbol == '0':
                self.state = 2
            elif symbol == '1':
                self.state = 1
        elif self.state == 2:
            if symbol == '0':
                self.state = 0
            elif symbol == '1':
                self.state = 1

    def accepts(self, string):
        for symbol in string:
            self.transition(symbol)
        return self.state == 2  



afd = AFD_TerminaEm01()
print(afd.accepts("1101"))  
print(afd.accepts("100"))   
print(afd.accepts("01"))    
print(afd.accepts("0101"))   
